{
  "name": "react-geoconnectable",
  "version": "1.0.0",
  "private": true,
  "author": "Dale MacDonald",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "csstools": "^0.1.5",
    "electron-is-dev": "^2.0.0",
    "google-map-react": "^2.1.10",
    "google-maps-react": "^2.0.6",
    "phidget22": "^2.8.1",
    "postcss": "^8.4.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-maps": "^9.4.5",
    "react-scripts": "4.0.3",
    "serve": "^13.0.2",
    "web-vitals": "^2.1.2"
  },
  "homepage": "./",
  "main": "public/electron-starter.js",
  "scripts": {
    "nfstart": "nf start -p 3000",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "startbiun": "./node_modules/react-scripts/bin/react-scripts.js --openssl-legacy-provider start",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "BROWSER=none react-scripts start",
    "react-serve": "BROWSER=none serve -s build",
    "react-build-start": "serve -s build -l 3000",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start-dev": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "concurrently \"cross-env BROWSER=none yarn react-serve\" \"wait-on http://localhost:5000 && electron .\""
  },
  "build": {
    "appId": "com.botherconsulting.react-geoconnectable",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "foreman": "^3.0.1",
    "wait-on": "^6.0.0"
  }
}
